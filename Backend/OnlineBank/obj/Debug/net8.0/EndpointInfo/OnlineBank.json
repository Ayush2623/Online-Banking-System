  "components": {
    "schemas": {
      "AuthDTO": {
        "required": [
          "mobileNumber",
          "role"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "role": {
            "minLength": 1,
            "pattern": "^(User|Admin)$",
            "type": "string"
          },
          "mobileNumber": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordRequest": {
        "type": "object",
        "properties": {
          "accountNumber": {
            "type": "integer",
            "format": "int64"
          },
          "oldPassword": {
            "type": "string",
            "nullable": true
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ForgotPasswordRequest": {
        "required": [
          "email",
          "mobileNumber"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "mobileNumber": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "pattern": "^[a-zA-Z0-9._%+-]+@gmail\\.com$",
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "ForgotUserIdRequest": {
        "required": [
          "email",
          "mobileNumber"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "mobileNumber": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "pattern": "^[a-zA-Z0-9._%+-]+@gmail\\.com$",
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "FundTransferRequest": {
        "type": "object",
        "properties": {
          "fromAccountNumber": {
            "type": "integer",
            "format": "int64"
          },
          "toAccountNumber": {
            "type": "integer",
            "format": "int64"
          },
          "amount": {
            "type": "number",
            "format": "double"
          },
          "transferMode": {
            "type": "string",
            "nullable": true
          },
          "remarks": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDTO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "NetBankingDTO": {
        "type": "object",
        "properties": {
          "netBankingId": {
            "type": "integer",
            "format": "int32"
          },
          "accountNumber": {
            "type": "integer",
            "format": "int64"
          },
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PayeeDTO": {
        "type": "object",
        "properties": {
          "payeeId": {
            "type": "integer",
            "format": "int32"
          },
          "payeeName": {
            "type": "string",
            "nullable": true
          },
          "payeeAccountNumber": {
            "type": "integer",
            "format": "int64"
          },
          "nickname": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PendingAccountDTO": {
        "required": [
          "aadharCardNumber",
          "accountType",
          "email",
          "mobileNumber",
          "name",
          "occupationDetails",
          "permanentAddress",
          "residentialAddress"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "pattern": "^[a-zA-Z0-9._%+-]+@gmail\\.com$",
            "type": "string",
            "format": "email"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "mobileNumber": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          },
          "aadharCardNumber": {
            "minLength": 1,
            "pattern": "^\\d{12}$",
            "type": "string"
          },
          "residentialAddress": {
            "minLength": 1,
            "type": "string"
          },
          "permanentAddress": {
            "minLength": 1,
            "type": "string"
          },
          "occupationDetails": {
            "minLength": 1,
            "type": "string"
          },
          "accountType": {
            "minLength": 1,
            "type": "string"
          },
          "balance": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "enableNetBanking": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "SetNewPasswordRequest": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          },
          "resetToken": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateAccountDTO": {
        "required": [
          "email",
          "mobileNumber",
          "name",
          "occupationDetails",
          "permanentAddress",
          "residentialAddress"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "pattern": "^[a-zA-Z0-9._%+-]+@gmail\\.com$",
            "type": "string",
            "format": "email"
          },
          "mobileNumber": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          },
          "residentialAddress": {
            "minLength": 1,
            "type": "string"
          },
          "permanentAddress": {
            "minLength": 1,
            "type": "string"
          },
          "occupationDetails": {
            "minLength": 1,
            "type": "string"
          },
          "enableNetBanking": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "UpdateNetBankingPasswordRequest": {
        "type": "object",
        "properties": {
          "accountId": {
            "type": "integer",
            "format": "int32"
          },
          "oldPassword": {
            "type": "string",
            "nullable": true
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Enter 'Bearer' [space] and then your valid JWT token.\n\nExample: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}